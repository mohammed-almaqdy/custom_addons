<odoo> 

  <record model="ir.ui.view" id="event_oolist">
        <field name="name">event.event.form</field>
        <field name="model">event.event</field>
        <field name="inherit_id" ref="event.view_event_form"/>
        <field name="arch" type="xml">
          <xpath expr="//field[@name='address_id']" position="after">
            <field name="theme_event" /> 
          </xpath>
          <xpath expr="//field[@name='organizer_id']" position="replace">
            <field name="organizer_id" invisible='1'/>
          </xpath> 
          <xpath expr="//page[@name='event_communication']" position="after">
            <page string="matercal equp">
              <group>
                <field name="material_tab" />
              </group>
            </page>
          </xpath>
          <xpath expr="//page[@name='event_notes']" position="after">
            <page string="event orginizer">
              <group>
                <field name="event_orgnizer" />
              </group>
            </page>
          </xpath>
        </field>       
  </record> 
<!-- event.event tree view -->
  <record id="name_view_tree" model="ir.ui.view">
    <field name="name">event.event.view.tree</field>
    <field name="model">event.event</field>
    <field name="inherit_id" ref="event.view_event_tree"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='address_id']" position="after">
        <field name="theme_event" />
      </xpath>
    </field>
  </record>
 <record model="ir.actions.act_window" id="event_list">
      <field name="name">Event</field>
      <field name="res_model">event.event</field>
      <field name="view_mode">tree,form</field>
 </record>
<menuitem id="main_events" name = 'My Events'/>
<menuitem id="part_events" parent='main_events' name = 'my events'/>
<menuitem id="see_events" parent='part_events' name = 'view events' action='event_list'/>
</odoo>